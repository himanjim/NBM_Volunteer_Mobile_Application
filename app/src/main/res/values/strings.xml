    <string name="permission_alert_title"><b>LOCATION PERMISSION REQUEST</b></string>
    <string name="permission_alert_msg">Dear user we need permission to access your precise <b>location</b> both in the foreground and <b>background</b>. We need your precise location to capture what is the mobile signal strength and related parameters at your location. Please note that <b>no information</b> related to your identity is captured. The data captured can never be backtracked to your identity.</string>
    <string name="permission_alert_ok">OK</string>

    <string name="bg_permission_alert_title"><b>BACKGROUND LOCATION PERMISSION REQUEST</b></string>
    <string name="bg_permission_alert_msg">Dear user, we need background permission to always access your precise <b>location</b>. We need your location in the background to capture the mobile signal strength and related parameters at your location every 15 mins.</string>
    <string name="bg_permission_alert_ok">OK</string>

    <string name="permission_alert_title_again">LOCATION PERMISSION REQUEST</string>
    <string name="permission_alert_msg_again">Dear user without location permission, this application <b>won\'t work</b>. So please rethink and give the permission again. Or else you can install the application for the time being and reconsider later. Please contact me at &my_email; for further doubts.</string>
